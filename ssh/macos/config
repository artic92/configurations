Host obc-dev.muc
  HostName obc-dev.muc
  User root

Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/work_key

#ariccio-tec account
Host github.com-ariccio-tec
	HostName github.com
	User git
	IdentityFile ~/.ssh/work_key

#ariccio account
Host github.com-ariccio
	HostName github.com
	User git
	IdentityFile ~/.ssh/private_key

